    "jest": "^24.5.0",
    "redis-mock": "^0.43.0",
    "ts-jest": "^24.0.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.3333",
    "yarn": "^1.13.0"
  }
