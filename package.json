    "apollo-server-express": "^2.4.8",
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "lodash.merge": "^4.6.1",
    "node-fetch": "^2.3.0",
    "redis": "^2.8.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/lodash.merge": "^4.6.6",
    "@types/node-fetch": "^2.1.6",
    "@types/react-test-renderer": "^16.8.1",
    "@types/redis": "^2.8.11",
