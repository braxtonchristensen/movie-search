{
  "name": "movie-search",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "yarn clean-client && yarn clean-server",
    "clean-client": "cd client && rm -rf node_modules && yarn",
    "clean-server": "rm -rf node_modules && yarn",
    "client": "cd client && yarn start",
    "generate": "cd client && yarn generate",
    "setup": "yarn install-client && yarn install-server",
    "storybook": "cd client && yarn storybook",
    "install-client": "cd client && yarn",
    "install-server": "yarn",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "prod": "tsc && node ./build/index.js",
    "server": "ts-node-dev --respawn --transpileOnly ./server/index.ts",
    "test": "yarn jest",
    "test-client": "cd client && yarn test",
    "tsc": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
